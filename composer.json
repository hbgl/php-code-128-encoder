{
    "name": "hbgl/php-code-128-encoder",
    "description": "Encode text for use with Code 128 fonts such as Libre Barcode 128.",
    "homepage": "https://github.com/hbgl/php-code-128-encoder",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Barcode",
        "Barcode Font",
        "Code 128",
        "Libre Barcode",
        "Encoder"
    ],
    "require": {
        "php": ">=8.0",
        "tecnickcom/tc-lib-barcode": ">=2.0.7 <=2.4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1.3||^9.5.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.16"
    },
    "autoload": {
        "psr-4": {
            "Hbgl\\Barcode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hbgl\\Barcode\\Tests\\": "tests/"
        }
    }
}
